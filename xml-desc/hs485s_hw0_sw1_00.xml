				<Element Value="0x00" Bezeichnung="Schalt-Ausgang 1" Default="1" ></Element>
				<Element Value="0x01" Bezeichnung="Schalt-Ausgang 2" ></Element>
			</Option>
		</Wert>
		<Wert Adresse="0x0011" Bezeichnung="Zuordnung Taster-Eingang T2" Type="Option" Laenge="1" Hilfe="3" >
