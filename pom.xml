<?xml version="1.0" encoding="UTF-8"?>
<project>
	<modelVersion>4.0.0</modelVersion>
	<name>HS485-Devices</name>
	<groupId>ch.eleveneye.hs485</groupId>
	<artifactId>devices</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<description>Ger√§te</description>
	<scm>
		<connection>
			scm:svn:http://venus.intern.eleveneye.ch/svn/eleveneye/trunk/HS485/HS485-Devices
		</connection>
	</scm>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0</version>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>xstream</groupId>
			<artifactId>xstream</artifactId>
			<version>1.1.3</version>
		</dependency>
		<dependency>
			<groupId>ch.eleveneye.hs485</groupId>
			<artifactId>comm</artifactId>
			<version>0.5.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>1.2.13</version>
		</dependency>
	</dependencies>
	<repositories>
		<repository>
			<releases>
				<enabled>false</enabled>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>warn</checksumPolicy>
			</releases>
			<snapshots>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>warn</checksumPolicy>
			</snapshots>
			<id>eleveneye-snapshot</id>
			<name>Eleveneye Snapshots</name>
			<url>http://venus.intern.eleveneye.ch/maven/snapshot</url>
			<layout>default</layout>
		</repository>
		<repository>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>warn</checksumPolicy>
			</releases>
			<snapshots>
				<enabled>false</enabled>
				<updatePolicy>always</updatePolicy>
				<checksumPolicy>warn</checksumPolicy>
			</snapshots>
			<id>eleveneye-release</id>
			<name>Eleveneye Release</name>
			<url>http://venus.intern.eleveneye.ch/maven/releases</url>
			<layout>default</layout>
		</repository>
	</repositories>
	<distributionManagement>
		<!-- use the following if you're not using a snapshot version.-->
		<repository>
			<id>eleveneye</id>
			<name>Repository Name</name>
			<url>file:///var/www/maven/release</url>
		</repository>
		<!-- use the following if you ARE using a snapshot version -->
		<snapshotRepository>
			<layout>default</layout>
			<id>eleveneye</id>
			<name>Repository Name</name>
			<url>file:///var/www/maven/snapshot</url>
		</snapshotRepository>
	</distributionManagement>
</project>